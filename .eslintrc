{
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-console": "off",
    "import/prefer-default-export": "off",
    "react/prop-types": 0,
    "no-nested-ternary": "off"
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "presets": [
      "@babel/preset-react"
    ],
    "requireConfigFile": false,
    "ecmaVersion": 2018,
    "sourceType": "module"
  }
}